{
  "name": "wakatime",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'pnpm run cypress:open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'pnpm run cypress:run'",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest run --coverage --environment jsdom"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "d3": "~7.8.2",
    "daisyui": "~2.50.0",
    "react-router-dom": "~6.8.0",
    "clsx": "~1.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "tailwindcss": "~3.2.4",
    "postcss": "~8.4.21",
    "autoprefixer": "~10.4.13",
    "@types/node": "~18.11.18",
    "cypress": "~12.5.1",
    "@testing-library/cypress": "~9.0.0",
    "start-server-and-test": "~1.15.3",
    "vitest": "~0.28.3",
    "@testing-library/react": "~13.4.0",
    "@testing-library/jest-dom": "~5.16.5",
    "jsdom": "~21.1.0"
  }
}
